<%-
  key = calculate_api_key(current_user.salt, "api/items", "create")
-%>
javascript:(function(){
var s=document.createElement('script');
s.src='<%= bookmarklet_url(:format => :js) %>';
s.addEventListener('load',function(){Items.bookmark(<%= current_user.id %>, '<%= key %>');});
document.body.appendChild(s);
})();
